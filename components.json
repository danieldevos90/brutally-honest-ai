{
    "$schema": "https://ui.shadcn.com/schema.json",
    "style": "default",
    "rsc": false,
    "tsx": true,
    "tailwind": {
      "config": "tailwind.config.js",
      "css": "src/styles/globals.css",
      "baseColor": "neutral",
      "cssVariables": true
    },
    "aliases": {
      "components": "@/components",
      "utils": "@/lib/utils"
    },
    "registries": {
      "@shadcnblocks": {
        "url": "https://shadcnblocks.com/r/{name}",
        "headers": {
          "Authorization": "Bearer ${SHADCNBLOCKS_API_KEY}"
        }
      }
    }
  }
  